#include <iostream>
#include <string>

using namespace std;

int main() {
    string placa;
    cout<<"Ingrese la placa : ";
    cin>>placa;
    cout<<"SOAT:";
    if(int(placa[4])%2 == 0)
        cout<<"Vigente";
    else
        cout<<"Vencido";
    cout<<endl<<"CITV:";
    if(int(placa[5])%2 == 0)
        cout<<"Vigente";
    else
        cout<<"Vencido";
    cout<<endl<<"AUTORIZADO:";
    if(int(placa[6] + placa[7])%3 == 0)
        cout<<"Si";
    else
        cout<<"No";
}
