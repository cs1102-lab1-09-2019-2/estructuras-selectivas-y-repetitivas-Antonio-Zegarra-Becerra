#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    int cantidad, num;
    vector<int> secuencia;
    cout<<"n: ";
    cin>>cantidad;
    for(int i = 0; i < cantidad; i++) {
        cin>>num;
        secuencia.push_back(num);
    }
    sort(secuencia.begin(), secuencia.end());
    cout<<"El valor maximo es: "<<secuencia[secuencia.size()-1];
}
